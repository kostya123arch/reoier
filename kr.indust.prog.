1.2 #include<iostream>
#include<vector>
using namespace std;

int main(){
    int c = 0, nc = 0;
    int n;
    cin >> n;
    vector<int> v(n);
    for (int i = 0; i < n; i++){
       cin >> v[i];
    }
    for(int i = 0; i < v.size();++i){
        if (i%2 == 0) c++;
       else nc++;
    }
    cout << "чётных: "<< c << endl << "нечётных: " << nc;
    return 0;
}
2.2 #include <iostream>
#include <vector>

using namespace std;

int main() {
	int n, m;

	cin >> n;
	cin >> m;

	vector<vector<int>> v(n, vector<int>(m));

	for (int i = 0; i < n; ++i) {
		for (int j = 0; j < n; ++j) {
			cin >> v[i][j];
		}
	}

	int sum = 0;
	for (int i = 0; i < n; ++i) {
		for (int j = 0; j < m; ++j) {
			sum += v[i][j];
		}
	}

	cout <<  sum << endl;

	return 0;
}
3.2 #include <iostream>
#include <vector>

using namespace std;

int max(vector<int> v) {


	int mx = v[0];
	for (int i = 1; i < v.size(); ++i) {
		if (v[i] > mx) {
			mx = v[i];
		}
	}
	return mx;
}

int main() {
	int n;
	cin >> n;
	vector<int> v(n);
	for (int i = 0; i < n; ++i) {
		cin >> v[i];
	}
	int mx = max(v);

	cout << mx << endl;

	return 0;
}
